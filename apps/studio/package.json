{
  "name": "studio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist-electron/main/main.js",
  "scripts": {
    "dev:react": "vite",
    "dev:electron": "cross-env NODE_ENV=development electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:electron": "tsc --project electron/tsconfig.json",
    "build:mac": "yarn run transpile:electron && yarn run build && electron-builder --mac --arm64",
    "build:win": "yarn run transpile:electron && yarn run build && electron-builder --win --x64",
    "build:linux": "yarn run transpile:electron && yarn run build && electron-builder --linux --x64"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.12",
    "lucide-react": "^0.541.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "cross-env": "^10.0.0",
    "electron": "^37.3.1",
    "electron-builder": "^26.0.12",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
